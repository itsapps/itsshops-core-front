---
permalink: /site.webmanifest
eleventyExcludeFromCollections: true
excludeFromSitemap: true
---
{
  "name": "{{ cms.getSiteSettings(settings.defaultLocale).siteTitle }} - {{ cms.getSiteSettings(settings.defaultLocale).siteShortDescription }}",
  "short_name": "{{ cms.getSiteSettings(settings.defaultLocale).siteTitle }}",
  "start_url": "{{ settings.toAbsoluteUrl(settings.defaultLocale) }}/",
  "icons": [
    {
      "src": "/web-app-manifest-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/web-app-manifest-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "theme_color": "{{ settings.manifestThemeColor }}",
  "background_color": "{{ settings.manifestThemeBgColor }}",
  "display": "standalone"
}
